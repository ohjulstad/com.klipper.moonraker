  "flow": {
    "triggers": [
      {
        "id": "printer-online",
        "title": {
          "en": "Printer Online"
        },
        "hint": {
          "en": "3D Printer came online"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      },
      {
        "id": "printer-offline",
        "title": {
          "en": "Printer Offline"
        },
        "hint": {
          "en": "3D printer is bacame offline"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      },
      {
        "id": "print-started",
        "title": {
          "en": "Print started"
        },
        "hint": {
          "en": "Printer started printing"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      },
      {
        "id": "print-completed",
        "title": {
          "en": "Print finished"
        },
        "hint": {
          "en": "Current Print has finised"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      },
      {
        "id": "print-cancelled",
        "title": {
          "en": "Print Cancelled"
        },
        "hint": {
          "en": "Cancelled current print"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      },
      {
        "id": "print-paused",
        "title": {
          "en": "Print has been paused"
        },
        "hint": {
          "en": "Current print has been paused"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      },
      {
        "id": "print-resumed",
        "title": {
          "en": "Print has been resumed"
        },
        "hint": {
          "en": "Current print has been resumed"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      },
      {
        "id": "print-error",
        "title": {
          "en": "Print stopped with error"
        },
        "tokens": [
          {
            "name": "error-message",
            "type": "string",
            "title": {
              "en": "Error message"
            }
          }
        ],
        "hint": {
          "en": "Current print has stopped with error"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      }
    ],
    "actions": [
      {
        "id": "run-gcode",
        "title": {
          "en": "Run GCode"
        },
        "titleFormated": {
          "en": "Run GCcode [[GCode]]"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          },
          {
            "type": "text",
            "name": "GCode",
            "placeholder": {
              "en": "G28"
            }
          }
        ]
      },
      {
        "id": "pause-print",
        "title": {
          "en": "Pause the print"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=moonraker"
          }
        ]
      }
    ]
  },
